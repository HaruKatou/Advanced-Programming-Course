#include <iostream>
#include <string>
#include <map>
#include <algorithm>
#include <vector>
using namespace std;
int main()
{
    map<int,map<int,int>> Map; // mảng 2 chiều có value mỗi key bằng 0
    int n; cin>>n;
    int a,b; cin>>a>>b; //In bước 1 trước để setup a_max và b_max
    for(int j=0;j<a;j++)
        for(int k=0;k<b;k++)
        {
            Map[j][k]++;
        }
        
    int a_max=a,b_max=b;
    
    for(int i=1;i<n;i++)
    {
        cin>>a>>b;
        if(a_max<a) a_max=a;
        if(b_max<b) b_max=b;
        for(int j=0;j<a;j++)
            for(int k=0;k<b;k++)
        {
            Map[j][k]++;
        }
    }
    
    int max_so=Map[0][0];  //Tìm vị trí X
    for(int i=0;i<a_max;i++)
        for(int j=0;j<b_max;j++)
    {
        if(max_so<Map[i][j])
            max_so=Map[i][j];
    }
    int dem=0;              //Tìm số lần X xuất hiện
    for(int i=0;i<a_max;i++)
        for(int j=0;j<b_max;j++)
            if(Map[i][j]==max_so)
                dem++;
    cout<<dem;
}
