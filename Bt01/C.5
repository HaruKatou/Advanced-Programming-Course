void doc_3so(int a,int b,int c)
{
    if(a!=0)
    {
        docdonvi(a);
        cout<<"hundred ";
    }
    if(b==1)
    {
        hangchuc_teen(c);
    }
    if(b==0)
    {
        if(c!=0)
            docdonvi(c);
    }
    if(b>1)
    {
        hangchuc(b);
        if(c!=0)
        {   cout<<'-';
            docdonvi(c);
        }
    }
}
