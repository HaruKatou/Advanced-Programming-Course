#include <iostream>
#include <iomanip>
#include <cmath>
using namespace std;
int main()
{
    int n;
    cin>>n;
    for(int y=1; y<=3 ;y++)
    {
        for(int x =-(n-1) ; x<=n-1 ;x++)
            if(abs(x)<y)
                cout<<'*';
            else
                cout<<' ';
        cout<<endl;
    }
}
