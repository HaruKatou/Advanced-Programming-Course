#include <iostream>
#include <iomanip>
#include <cmath>
#include <vector>
using namespace std;
int daoso(int n)
{
    int res=0;
    int temp=0;
    int cnt=0;
    while(n)
    {
        temp=n%10;
        res=res*10+temp;
        n=n/10;
    }
    return res;
}
int main()
{
    int n;
    cin>>n;
    int a,b;
    int dem=0;
    for(int j=0;j<n;j++)
    {
        dem=0;
        cin>>a>>b;
        for(int i=a;i<=b;i++)
        {
            if(i==daoso(i))
            {
                dem++;
            }
        }
        cout<<dem<<endl;
    }

}
